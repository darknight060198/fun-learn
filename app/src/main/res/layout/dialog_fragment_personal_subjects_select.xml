<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/main_activity_student_dialog_personal_select_subject_bi"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Bahasa Indonesia"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="2.5dp"
        android:background="@drawable/background_profil_username"/>

    <TextView
        android:id="@+id/main_activity_student_dialog_personal_select_subject_bing"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Bahasa Inggris"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="2.5dp"
        android:background="@drawable/background_profil_username"/>

    <TextView
        android:id="@+id/main_activity_student_dialog_personal_select_subject_ipa"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Ilmu Pengetahuan Alam"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="2.5dp"
        android:background="@drawable/background_profil_username"/>

    <TextView
        android:id="@+id/main_activity_student_dialog_personal_select_subject_ips"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Ilmu Pengetahuan Sosial"
        android:textAlignment="center"
        android:textSize="18sp" />

    <TextView
        android:layout_width="200dp"
        android:layout_height="2.5dp"
        android:background="@drawable/background_profil_username"/>

    <TextView
        android:id="@+id/main_activity_student_dialog_personal_select_subject_mat"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="Matematika"
        android:textAlignment="center"
        android:textSize="18sp" />

</LinearLayout>