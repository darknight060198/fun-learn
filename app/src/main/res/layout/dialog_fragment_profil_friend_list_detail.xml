<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minHeight="300dp"
    android:minWidth="275dp">

    <android.support.constraint.Guideline
        android:id="@+id/guideline17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.43" />

    <android.support.constraint.Guideline
        android:id="@+id/guideline18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.33" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/dialog_fragment_profile_friend_list_detail_fullname"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17" />

    <ImageView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_photo"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/background_profil_username"
        app:layout_constraintBottom_toTopOf="@+id/guideline18"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_games"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/main_activity_student_profile_statistic_total_games"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toTopOf="@+id/guideline18" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_win"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/main_activity_student_profile_statistic_total_win"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_games" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_lose"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="@string/main_activity_student_profile_statistic_total_lose"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_win" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_games_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toTopOf="@+id/guideline18" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_win_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_games_score" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_lose_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_win_score" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_best_pvp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="@string/main_activity_student_profile_statistic_best_pvp_score"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_lose" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_best_pvp_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_lose_score" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_exercise_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_best_time_trial_score" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_exercise"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/main_activity_student_profile_statistic_total_exercise"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_best_time_trial" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_best_time_trial_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_best_pvp_score" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_best_time_trial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="@string/main_activity_student_profile_statistic_best_time_trial_score"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_best_pvp" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_learn_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_exercise_score" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_total_learn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:text="@string/main_activity_student_profile_statistic_total_time_learn"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_exercise" />

    <TextView
        android:id="@+id/dialog_fragment_profile_friend_list_detail_fullname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginLeft="32dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline18"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_learn" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline17"
        app:layout_constraintTop_toBottomOf="@+id/dialog_fragment_profile_friend_list_detail_total_learn_score" />

</android.support.constraint.ConstraintLayout>
